    "@types/pify": "^5.0.1",
    "@types/uniqid": "^5.3.2",
    "@types/uuid": "^9.0.2",
    "await-to-js": "^3.0.0",
    "better-sqlite3": "^8.4.0",
    "class-transformer": "^0.5.1",
